{
  "name": "hello-heroku-ts",
  "version": "1.0.0",
  "description": "",
  "main": "dist/server.js",
  "scripts": {
    "build-ts": "tsc",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "postinstall": "npm run build-ts",
    "start": "npm run serve",
    "serve": "node dist/server.js",
    "test": "jest src",
    "watch-node": "nodemon dist/server.js",
    "watch-ts": "tsc -w"
  },
  "author": "",
  "license": "ISC",
  "engines": {
    "node": "8.11.4"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/jest": "^23.3.1",
    "@types/multer-s3": "^2.7.7",
    "jest": "^25.0.0",
    "nodemon": "^1.18.3",
    "prettier": "^1.14.2",
    "supertest": "^3.1.0",
    "ts-jest": "^23.1.4",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-plugin-prettier": "^1.3.0",
    "typescript": "^3.0.1",
    "@types/body-parser": "^1.17.1",
    "@types/express": "^4.17.1",
    "@types/jsonwebtoken": "^8.3.3",
    "@types/multer": "^1.3.9",
    "@types/pg": "^7.4.10"
  },
  "dependencies": {
    "aws-sdk": "^2.543.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "pg": "^7.4.3",
    "randomstring": "^1.1.5",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.7.7",
    "typeorm": "^0.2.18",
